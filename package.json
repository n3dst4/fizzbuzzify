{
  "name": "fizzbuzzify",
  "version": "2.0.2",
  "description": "Turn numbers into their corresponding fizzbuzz result",
  "author": "Neil de Carteret <n3dst4@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/n3dst4/fizzbuzzify",
  "main": "__build/fizzbuzzify.js",
  "scripts": {
    "prepublish": "babel src --out-dir __build --source-maps inline",
    "watch": "babel --watch src --out-dir __build --source-maps inline",
    "test": "mocha"
  },
  "browserify": {
    "transform": []
  },
  "dependencies": {
    "core-js": "^3.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babelify": "^10.0.0",
    "chai": "^3.2.0",
    "envify": "^3.4.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^3.2.2",
    "mocha": "^6.1.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
      "node": true
    },
    "rules": {
      "quotes": 0,
      "comma-dangle": 0,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ]
    }
  }
}
